#include <iostream>
using namespace std;

int remDups(int arr[], int n) {
    if (n == 0) return 0;

    int temp[n];
    temp[0] = arr[0];
    int res = 1;

  
    for (int i = 1; i < n; i++) {
        if (temp[res - 1] != arr[i]) {
            temp[res] = arr[i];
            res++;
        }
    }

    
    for (int i = 0; i < res; i++) {
        arr[i] = temp[i];
    }

    return res;
}

int main() {
    int arr[] = {10, 20, 20, 30, 30, 30, 40};
    int n = sizeof(arr) / sizeof(arr[0]);

    int newSize = remDups(arr, n);

    cout << "Array after removing duplicates: ";
    for (int i = 0; i < newSize; i++) {
        cout << arr[i] << " ";
    }
    cout << endl;

    return 0;
}
